{
    "name": "ancarda/functional",
    "description": "Functional Programming Helper Functions and Classes",
    "keywords": [
        "immutable",
        "functional"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mark Dain",
            "email": "mark@markdain.net",
            "homepage": "https://markdain.net",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "platform": {
            "php": "7.0"
        }
    },
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "squizlabs/php_codesniffer": "^3.2",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs",
        "fix-style": "phpcbf"
    },
    "autoload": {
        "psr-4": {
            "Ancarda\\Functional\\": "src"
        }
    }
}
